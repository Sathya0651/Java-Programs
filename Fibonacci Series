public class Fibanocci
{
    public static void main(String[] arg)
    {
        int n = 10;
        int first = 0;
        int second = 1;
        System.out.print("Fibanocci Series: ");
        for(int i = 1; i <= n; i++)
        {
            System.out.print(" " + first);
            int next = first + second;
            first = second;
            second = next;
        }
    }
}
