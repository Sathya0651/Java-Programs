public class Rectangle
{
    public static void main(String[] arg)
    {
        int r=5;
        int c=10;
        for(int i=1;i<=r;i++)
        {
            for(int j=1;j<=c;j++)
            {
                System.out.print(" * ");
            }
            System.out.println();
        }
    }
}
