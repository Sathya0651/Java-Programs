class r192210651
{
    public static void main(String[] arg)
    {
        int n = 28;
        System.out.println("The factors of "+ n + " are ");
        for(int i=1;i<=n;i++)
        {
            if(n % i == 0)
            {
                System.out.print(" "+i);
            }
        }
    }
}
