public class Arthematic
{
    public static void main(String[] args)
    {
        int a=25,b=5;
        int A = a+b;
        int S = a-b;
        int M = a*b;
        int D = a/b;
        System.out.println("Addition: "+ A);
        System.out.println("Subtraction: "+ S);
        System.out.println("Multiplication: "+ M);
        System.out.println("Division: "+ D);
    }
}
