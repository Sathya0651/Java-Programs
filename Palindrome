public class PalindromeCheck 
{
    public static void main(String[] args) 
    {
        String s1 = "121";
        String s2 = "";
        int len = s1.length();
        for(int i = len-1;i>=0;i--)
        {
            s2 = s2+s1.charAt(i);
        }
        if(s1.equals(s2))
        {
            System.out.print("Its a palindrome");
        }
        else
        {
            System.out.print("Its not a palindrome");
        }
    }
}
